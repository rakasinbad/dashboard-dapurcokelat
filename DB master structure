-- master.promopos_bundling_produk definition

CREATE TABLE `promopos_bundling_produk` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `promo_code` varchar(255) NOT NULL DEFAULT '',
  `bundling_code` varchar(255) NOT NULL DEFAULT '',
  `item_code` varchar(255) NOT NULL DEFAULT '',
  `parent` varchar(255) NOT NULL DEFAULT '',
  `qty` int(11) NOT NULL DEFAULT 1,
  `price` int(11) NOT NULL DEFAULT 0,
  `status` int(1) NOT NULL DEFAULT 1,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=680 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;


-- master.promopos_coupon definition

CREATE TABLE `promopos_coupon` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `promopos_id` int(11) DEFAULT NULL,
  `database_id` int(11) DEFAULT 6,
  `storage_transaksi_id` bigint(20) DEFAULT NULL,
  `no_trx` varchar(255) DEFAULT '',
  `coupon_code` varchar(100) DEFAULT NULL,
  `created_at` datetime DEFAULT current_timestamp(),
  `send_wa` int(1) DEFAULT 0,
  PRIMARY KEY (`id`),
  KEY `promopos_coupon_database_id_IDX` (`database_id`) USING BTREE,
  KEY `promopos_coupon_storage_transaksi_id_IDX` (`storage_transaksi_id`) USING BTREE,
  KEY `promopos_coupon_coupon_code_IDX` (`coupon_code`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=90 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;


-- master.promopos_coupon_struk definition

CREATE TABLE `promopos_coupon_struk` (
