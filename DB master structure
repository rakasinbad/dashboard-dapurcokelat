  `status_pajak` varchar(64) NOT NULL DEFAULT '',
  `no_pengajuan_pajak` varchar(64) NOT NULL DEFAULT '',
  `order_sn_third_party` varchar(225) NOT NULL DEFAULT '',
  UNIQUE KEY `id` (`id`),
  KEY `outletnstatus` (`status`,`outletpengirim`,`created`),
  KEY `paymentmethod` (`paymentmethod`),
  KEY `namakasir` (`namakasir`),
  KEY `phonecustomer` (`phonecustomer`),
  KEY `kodeorder` (`kodeorder`),
  KEY `trx_no` (`trx_no`),
  KEY `kode_promo` (`kode_promo`),
  KEY `deldate` (`deldate`),
  KEY `faktur_pajak` (`status_pajak`,`no_pengajuan_pajak`)
