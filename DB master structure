  `origin_lng` varchar(24) NOT NULL DEFAULT '',
  `destination_lat` varchar(24) NOT NULL DEFAULT '',
  `destination_lng` varchar(24) NOT NULL DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=952 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;


