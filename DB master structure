  `kelurahan` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `zipcode` varchar(10) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `outletpengirim` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `kotadelivery` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `cash` varchar(10) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `dateread` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `readby` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `dateprint` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `printby` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `datereceived` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `receivedby` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `delman` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
  `datetrx` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `tgl_trx` date NOT NULL,
  `trxby` varchar(50) CHARACTER SET latin1 COLLATE latin1_swedish_ci NOT NULL,
