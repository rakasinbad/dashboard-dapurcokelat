-- master.itemcode definition

CREATE TABLE `itemcode` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `kategoriutama` char(1) NOT NULL DEFAULT '',
  `kategori` varchar(250) NOT NULL DEFAULT '',
  `item` varchar(250) NOT NULL DEFAULT '',
  `name` varchar(250) NOT NULL DEFAULT '',
  `buy_unit` varchar(250) NOT NULL DEFAULT '',
  `buy_packing` varchar(250) NOT NULL DEFAULT '',
  `buy_content` varchar(250) NOT NULL DEFAULT '0',
  `cost_unit` varchar(250) NOT NULL DEFAULT '',
  `konversi` varchar(250) NOT NULL DEFAULT '1',
  `req_unit` varchar(250) NOT NULL DEFAULT '',
  `req_packing` varchar(250) NOT NULL DEFAULT '',
  `convusage` varchar(10) NOT NULL DEFAULT '',
  `unitusage` varchar(250) NOT NULL DEFAULT '',
  `status` varchar(250) NOT NULL DEFAULT '',
  `buaran` char(1) NOT NULL DEFAULT '0',
  `kranggan` char(1) NOT NULL DEFAULT '0',
  `surabaya` char(1) NOT NULL DEFAULT '0',
  `pekanbaru` char(1) NOT NULL DEFAULT '0',
  `makassar` char(1) NOT NULL DEFAULT '0',
  `edited_datetime` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `editedby` varchar(250) NOT NULL DEFAULT '',
  PRIMARY KEY (`id`),
  KEY `item` (`item`),
  KEY `status` (`status`),
  KEY `buaran` (`buaran`),
  KEY `kranggan` (`kranggan`),
  KEY `surabaya` (`surabaya`),
  KEY `pekanbaru` (`pekanbaru`),
  KEY `makassar` (`makassar`),
  KEY `kategoriutama` (`kategoriutama`)
) ENGINE=InnoDB AUTO_INCREMENT=1136 DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;


