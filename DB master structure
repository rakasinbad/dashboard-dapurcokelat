-- master.promopos definition

CREATE TABLE `promopos` (
  `idpromo` int(11) NOT NULL AUTO_INCREMENT,
  `nama` varchar(64) NOT NULL DEFAULT 'nama promo',
  `target` varchar(128) NOT NULL DEFAULT 'prosespromo.php',
  `outlet` varchar(500) NOT NULL DEFAULT 'XX,',
  `region` varchar(200) NOT NULL DEFAULT '',
  `kode_area` varchar(255) NOT NULL DEFAULT '',
  `keterangan` varchar(512) NOT NULL DEFAULT '',
  `validstr` date NOT NULL DEFAULT '0000-00-00',
  `validto` date NOT NULL DEFAULT '0000-00-00',
  `jenis` varchar(20) NOT NULL DEFAULT 'custom',
  `idcustomer` varchar(20) NOT NULL DEFAULT '',
  `ket` varchar(32) NOT NULL DEFAULT '',
  `nilai` varchar(20) NOT NULL DEFAULT '',
  `status` int(1) NOT NULL DEFAULT 1,
  `kategori_promo` char(1) NOT NULL DEFAULT '1',
  `management_outlet` varchar(3) NOT NULL DEFAULT '',
  `stack` tinyint(1) NOT NULL DEFAULT 0,
  `kelipatan` varchar(6) NOT NULL DEFAULT '',
  `usage` int(11) NOT NULL DEFAULT 0,
  `is_all_item` varchar(20) NOT NULL DEFAULT '',
  `type_promo` varchar(255) NOT NULL DEFAULT '',
  `benefit_all_item_type` varchar(40) NOT NULL DEFAULT '',
  `benefit_all_item_value` double NOT NULL DEFAULT 0,
  `benefit_max` double NOT NULL DEFAULT 0,
  `layer` varchar(50) NOT NULL DEFAULT '',
  `prerequisite_promo` varchar(50) NOT NULL DEFAULT '',
  `maf_id` varchar(255) NOT NULL DEFAULT '',
  `created_at` timestamp NOT NULL DEFAULT current_timestamp(),
  `created_by` varchar(20) NOT NULL DEFAULT '',
  `updated_at` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' ON UPDATE current_timestamp(),
  `updated_by` varchar(50) NOT NULL DEFAULT '',
  `condition_all_type` varchar(50) NOT NULL DEFAULT '',
  `condition_all_value` double NOT NULL DEFAULT 0,
  `pre_condition` varchar(100) NOT NULL DEFAULT '',
  `pre_condition_value` varchar(100) NOT NULL DEFAULT '',
  `double_stack` varchar(10) NOT NULL DEFAULT '',
  `promo_day` varchar(5) NOT NULL DEFAULT '',
  `nama_promo` varchar(100) NOT NULL DEFAULT '',
  PRIMARY KEY (`idpromo`)
) ENGINE=InnoDB AUTO_INCREMENT=2508 DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;


