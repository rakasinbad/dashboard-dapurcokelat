  `id` int(11) NOT NULL AUTO_INCREMENT,
  `kodeorder` varchar(50) NOT NULL DEFAULT '',
  `shipper_id` char(3) NOT NULL DEFAULT '',
  `delivery_id` varchar(50) NOT NULL DEFAULT '',
