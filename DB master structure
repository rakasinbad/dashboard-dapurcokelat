-- master.outlet definition

CREATE TABLE `outlet` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `kodeoutlet` char(10) NOT NULL DEFAULT '',
  `namaoutlet` varchar(50) NOT NULL DEFAULT '',
  `guid` varchar(50) NOT NULL DEFAULT '',
  `kota` char(2) NOT NULL DEFAULT '',
  `produksi` char(1) NOT NULL DEFAULT '',
  `warehouse` char(1) NOT NULL DEFAULT '',
  `delivery` varchar(2) NOT NULL DEFAULT '',
  `franchise` char(1) NOT NULL DEFAULT '',
  `jenis_outlet` char(2) NOT NULL DEFAULT '0',
  `posweb` char(1) NOT NULL DEFAULT '0',
  `status` char(1) NOT NULL DEFAULT '1',
  `parent` varchar(4) NOT NULL DEFAULT '',
  `am` varchar(26) NOT NULL DEFAULT '',
  `kodedb_id` char(1) NOT NULL DEFAULT '',
  `npwp` varchar(100) NOT NULL DEFAULT '',
  `status_cb` char(1) NOT NULL DEFAULT '',
  `spv` varchar(15) NOT NULL DEFAULT '',
  `status_report` char(1) NOT NULL DEFAULT '1',
  `tgl_buka` date NOT NULL DEFAULT '0000-00-00',
  `tgl_tutup` date NOT NULL DEFAULT '0000-00-00',
  UNIQUE KEY `id` (`id`),
  KEY `kodeoutlet` (`kodeoutlet`),
  KEY `gabung` (`kodeoutlet`,`produksi`),
  KEY `produksi` (`produksi`,`status`),
  KEY `kota` (`kota`,`status`)
) ENGINE=InnoDB AUTO_INCREMENT=244 DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;


