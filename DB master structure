-- master.bs definition

CREATE TABLE `bs` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `item` varchar(250) NOT NULL DEFAULT '',
  `brand` varchar(250) NOT NULL DEFAULT '',
  `disc` varchar(250) NOT NULL DEFAULT '0',
  `price` varchar(250) NOT NULL DEFAULT '0',
  `supplier` varchar(250) NOT NULL DEFAULT '',
  `defaul` varchar(10) NOT NULL DEFAULT '',
  `produksi` varchar(20) NOT NULL DEFAULT '',
  `namabarang` varchar(140) NOT NULL DEFAULT '',
  `produsen` varchar(64) NOT NULL DEFAULT '',
  `negara` varchar(50) NOT NULL DEFAULT '',
  `halal` varchar(50) NOT NULL DEFAULT '',
  `halal_cert` varchar(64) NOT NULL DEFAULT '',
  `expdate_sjh` date NOT NULL DEFAULT '0000-00-00',
  KEY `id` (`id`),
  KEY `defaul` (`defaul`,`produksi`),
  KEY `produksi` (`produksi`),
  KEY `item` (`item`)
) ENGINE=InnoDB AUTO_INCREMENT=8049 DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;


